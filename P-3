#include <stdio.h>

#define SIZE 5   

char queue[SIZE];
int front = -1, rear = -1;

void enqueue(char data) {
    if (rear == SIZE - 1) {
        printf("UART Buffer Overflow! Cannot receive '%c'\n", data);
    } else {
        if (front == -1)
            front = 0;
        rear++;
        queue[rear] = data;
        printf("Received: %c\n", data);
    }
}

void dequeue() {
    if (front == -1 || front > rear) {
        printf("UART Buffer Underflow! No data to read\n");
    } else {
        printf("Transmitted: %c\n", queue[front]);
        front++;
    }
}

// Display queue contents
void display() {
    if (front == -1 || front > rear) {
        printf("UART Buffer is empty\n");
    } else {
        printf("UART Buffer: ");
        for (int i = front; i <= rear; i++) {
            printf("%c ", queue[i]);
        }
        printf("\n");
    }
}

int main() {
 
    enqueue('A');
    enqueue('B');
    enqueue('C');
    enqueue('D');
    enqueue('E');

    enqueue('F');

    display();

    dequeue();
    dequeue();
    dequeue();
    dequeue();
    dequeue();

    dequeue();

    return 0;
}
